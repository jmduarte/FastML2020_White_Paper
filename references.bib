@article{Lai_2020,
	doi = {10.1088/1748-0221/15/06/c06063},
	url = {https://doi.org/10.1088/1748-0221/15/06/c06063},
	year = 2020,
	month = {jun},
	publisher = {{IOP} Publishing},
	volume = {15},
	number = {06},
	pages = {C06063--C06063},
	author = {Y.-T. Lai and M. Aoyama and H. Bae and S. Bähr and M.-C. Chang and H. Hayashii and Y. Iwasaki and J.-B. Kim and K.-T. Kim and C. Kiesling and T. Koga and P.-C. Lu and S.-M. Liu and F. Meggendorfer and H.-K. Moon and T.-J. Moon and H. Nakazawa and P. Rados and A. Rostomyan and J.-G. Shiu and S. Skambraks and T.-A. Sheng and Y. Sue and K. Unger and C.-H. Wang and E. Won and J. Yin},
	title = {Development of the Level-1 track trigger with Central Drift Chamber detector in Belle {II} experiment and its performance in {SuperKEKB} 2019 Phase 3 operation},
	journal = {Journal of Instrumentation},
	abstract = {The Level-1 trigger system (TRG) in the Belle II experiment is designed to summarize real-time sub-detectors information by using FPGA chips for the central data acquisition (DAQ) system, and it includes several sub-trigger systems for triggering various types of physics events in interest. The main focus in this report is CDCTRG: the track trigger system with Central Drift Chamber (CDC) detector, which is responsible for the real-time trajectory reconstruction of charged particles with various algorithms: Track-Segment Finder, 2D, 3D, Neuro-3D, and short tracking. CDCTRG is necessary for specific types of physics, such as hadronic and μ pair. This paper will introduce the design of the hardware system and algorithm implementation in FPGAs. Development, validation and performance of each CDCTRG module during SuperKEKB beam collision operation will also be discussed in detail.}
}

