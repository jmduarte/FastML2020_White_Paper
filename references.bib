@article{Kim_2015,
	doi = {10.1088/0264-9381/32/24/245002},
	url = {https://doi.org/10.1088/0264-9381/32/24/245002},
	year = 2015,
	month = {nov},
	publisher = {{IOP} Publishing},
	volume = {32},
	number = {24},
	pages = {245002},
	author = {Kyungmin Kim and Ian W Harry and Kari A Hodge and Young-Min Kim and Chang-Hwan Lee and Hyun Kyu Lee and John J Oh and Sang Hoon Oh and Edwin J Son},
	title = {Application of artificial neural network to search for gravitational-wave signals associated with short gamma-ray bursts},
	journal = {Classical and Quantum Gravity},
	abstract = {We apply a machine learning algorithm, the artificial neural network, to the search for gravitational-wave signals associated with short gamma-ray bursts (GRBs). The multi-dimensional samples consisting of data corresponding to the statistical and physical quantities from the coherent search pipeline are fed into the artificial neural network to distinguish simulated gravitational-wave signals from background noise artifacts. Our result shows that the data classification efficiency at a fixed false alarm probability (FAP) is improved by the artificial neural network in comparison to the conventional detection statistic. Specifically, the distance at 50% detection probability at a fixed false positive rate is increased about 8%–14% for the considered waveform models. We also evaluate a few seconds of the gravitational-wave data segment using the trained networks and obtain the FAP. We suggest that the artificial neural network can be a complementary method to the conventional detection statistic for identifying gravitational-wave signals related to the short GRBs.}
}

